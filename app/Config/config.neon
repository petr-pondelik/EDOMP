	repository.validation.function:
	    class: App\Model\Persistent\Repository\ValidationFunctionRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ValidationFunction

	repository.filter:
	    class: App\Model\Persistent\Repository\FilterRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Filter

	# Functionalities
	functionality.user: App\Model\Persistent\Functionality\UserFunctionality
	functionality.problem.final: App\Model\Persistent\Functionality\ProblemFinal\ProblemFinalFunctionality
	functionality.problem.final.equation.linear: App\Model\Persistent\Functionality\ProblemFinal\LinearEquationFinalFunctionality
	functionality.problem.final.equation.quadratic: App\Model\Persistent\Functionality\ProblemFinal\QuadraticEquationFinalFunctionality
	functionality.problem.final.sequence.arithmetic: App\Model\Persistent\Functionality\ProblemFinal\ArithmeticSequenceFinalFunctionality
	functionality.problem.final.sequence.geometric: App\Model\Persistent\Functionality\ProblemFinal\GeometricSequenceFinalFunctionality
	functionality.category: App\Model\Persistent\Functionality\CategoryFunctionality
	functionality.subcategory: App\Model\Persistent\Functionality\SubCategoryFunctionality
	functionality.problem: App\Model\Persistent\Functionality\ProblemFunctionality
	functionality.problem.type: App\Model\Persistent\Functionality\ProblemTypeFunctionality
	functionality.problem.final.test.variant.association: App\Model\Persistent\Functionality\ProblemFinalTestVariantAssociationFunctionality
	functionality.template.equation.linear: App\Model\Persistent\Functionality\ProblemTemplate\LinearEquationTemplateFunctionality
	functionality.template.equation.quadratic: App\Model\Persistent\Functionality\ProblemTemplate\QuadraticEquationTemplateFunctionality
	functionality.template.sequence.arithmetic: App\Model\Persistent\Functionality\ProblemTemplate\ArithmeticSequenceTemplateFunctionality
	functionality.template.sequence.geometric: App\Model\Persistent\Functionality\ProblemTemplate\GeometricSequenceTemplateFunctionality
	functionality.supergroup: App\Model\Persistent\Functionality\SuperGroupFunctionality
	functionality.group: App\Model\Persistent\Functionality\GroupFunctionality
	functionality.logo: App\Model\Persistent\Functionality\LogoFunctionality
	functionality.test: App\Model\Persistent\Functionality\TestFunctionality
	functionality.test.variant: App\Model\Persistent\Functionality\TestVariantFunctionality
	functionality.template.jsondata: App\Model\Persistent\Functionality\TemplateJsonDataFunctionality
	functionality.filter: App\Model\Persistent\Functionality\FilterFunctionality

	# Factories
	factory.header.bar: App\CoreModule\Components\HeaderBar\IHeaderBarFactory
	factory.side.bar: App\CoreModule\Components\SideBar\ISideBarFactory
	factory.form.user: App\Components\Forms\UserForm\IUserFormFactory
	factory.form.category: App\Components\Forms\CategoryForm\ICategoryFormFactory
	factory.form.subcategory: App\Components\Forms\SubCategoryForm\ISubCategoryFormFactory
	factory.form.problem.final: App\Components\Forms\ProblemFinalForm\IProblemFinalFormFactory
	factory.form.supergroup: App\Components\Forms\SuperGroupForm\ISuperGroupIFormFactory
	factory.form.logo: App\Components\Forms\LogoForm\ILogoIFormFactory
	factory.form.group: App\Components\Forms\GroupForm\IGroupIFormFactory
	factory.form.test: App\Components\Forms\TestForm\TestEntityForm\ITestFormFactory
	factory.form.permission: App\Components\Forms\PermissionForm\IPermissionIFormFactory
	factory.form.sign: App\CoreModule\Components\Forms\SignForm\ISignIFormFactory
	factory.form.problem.filter: App\Components\Forms\ProblemFilterForm\IProblemFilterFormFactory
	factory.grid.user: App\Components\DataGrids\UserGridFactory
	factory.grid.category: App\Components\DataGrids\CategoryGridFactory
	factory.grid.subcategory: App\Components\DataGrids\SubCategoryGridFactory
	factory.grid.problem: App\Components\DataGrids\ProblemGridFactory
	factory.grid.problem.type: App\Components\DataGrids\ProblemTypeGridFactory
	factory.grid.template: App\Components\DataGrids\TemplateGridFactory
	factory.grid.logo: App\Components\DataGrids\LogoGridFactory
	factory.grid.supergroup: App\Components\DataGrids\SuperGroupGridFactory
	factory.grid.group: App\Components\DataGrids\GroupGridFactory
	factory.grid.test: App\Components\DataGrids\TestGridFactory
	factory.logo.draganddrop: App\Components\LogoDragAndDrop\ILogoDragAndDropFactory
	factory.problem.stack: App\Components\ProblemStack\IProblemStackFactory
	factory.section.help.modal: App\CoreModule\Components\HelpModal\ISectionHelpModalFactory
	factory.view.logo: App\Components\LogoView\ILogoViewFactory
	factory.view.filter: App\Components\FilterTable\IFilterViewFactory
	factory.form.password: App\CoreModule\Components\Forms\PasswordForm\IPasswordFormFactory
	factory.form.test.template: App\Components\Forms\TestTemplateForm\ITestTemplateFormFactory

	# Services
	service.mail: App\Services\MailService(..., ..., %templatesDir%, %loginURL%)
	service.validator: App\Services\Validator
	service.file: App\Services\FileService(%logosDir%, %logosTmpDir%, %templatesDir%)
	service.authenticator: App\Services\Authenticator
	service.authorizator: App\Services\Authorizator
	service.eos.parser: jlawrence\eos\Parser
	service.session.problem.template: App\Services\ProblemTemplateSession
	service.session.filter: App\Services\FilterSession

	# Managers
	manager.entity.constraint: App\Model\Persistent\Manager\ConstraintEntityManager
	manager.homepage.statistics: App\Model\Persistent\Manager\HomepageStatisticsManager

	# Helpers
	helper.const: App\CoreModule\Helpers\ConstHelper
	helper.strings: App\CoreModule\Helpers\StringsHelper
	helper.latex: App\CoreModule\Helpers\LatexHelper
	helper.parser.newton: App\Helpers\NewtonParser
	helper.formatter: App\CoreModule\Helpers\FormatterHelper
	helper.flashes.translator: App\CoreModule\Helpers\FlashesTranslator
	helper.regular.expressions: App\CoreModule\Helpers\RegularExpressions
	helper.filter.view: App\Helpers\FilterViewHelper
	helper.generator.test: App\Helpers\TestGeneratorHelper