parameters:
    imagesPath: 'resources/images/'
    testMaxProblems: 20

extensions:
    # Vendor extensions
    console: Kdyby\Console\DI\ConsoleExtension
    events: Kdyby\Events\DI\EventsExtension
    annotations: Kdyby\Annotations\DI\AnnotationsExtension
    translation: Kdyby\Translation\DI\TranslationExtension
    validator: Kdyby\Validator\DI\ValidatorExtension
    doctrine: Kdyby\Doctrine\DI\OrmExtension
    assetMacro: Webrouse\AssetMacro\DI\Extension
    visualPaginator: IPub\VisualPaginator\DI\VisualPaginatorExtension
    webpack: Oops\WebpackNetteAdapter\DI\WebpackExtension(%debugMode%)
    ajaxHistory: VojtechDobes\NetteAjax\HistoryExtension

    # Own extensions
    teacherModule: App\TeacherModule\DI\TeacherModuleExtension

application:
	errorPresenter: Error
	mapping:
		*: App\*Module\Presenters\*Presenter

session:
	expiration: 14 days

# Set tracy max lenght and depth for dumps
tracy:
    maxLength: 2000
    maxDepth: 5

services:
	router: App\RouterFactory::createRouter

	# Repositories
	repository.user:
	    class: App\Model\Persistent\Repository\UserRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\User

	repository.role:
	    class: App\Model\Persistent\Repository\RoleRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Role

	repository.problem:
	    class: App\Model\Persistent\Repository\ProblemRepository
	    tags:
	       doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Problem

	repository.problem.final:
	    class: App\Model\Persistent\Repository\ProblemFinal\ProblemFinalRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemFinal\ProblemFinal

	repository.problem.final.equation.linear:
	    class: App\Model\Persistent\Repository\ProblemFinal\LinearEquationFinalRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemFinal\LinearEquationFinal

	repository.problem.final.equation.quadratic:
	    class: App\Model\Persistent\Repository\ProblemFinal\QuadraticEquationFinalRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemFinal\QuadraticEquationFinal

	repository.problem.final.sequence.arithmetic:
	    class: App\Model\Persistent\Repository\ProblemFinal\ArithmeticSequenceFinalRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemFinal\ArithmeticSequenceFinal

	repository.problem.final.sequence.geometric:
	    class: App\Model\Persistent\Repository\ProblemFinal\GeometricSequenceFinalRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemFinal\GeometricSequenceFinal

	repository.problem.template:
	    class: App\Model\Persistent\Repository\ProblemTemplate\ProblemTemplateRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemTemplate\ProblemTemplate

	repository.problemtype:
	    class: App\Model\Persistent\Repository\ProblemTypeRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemType

	repository.difficulty:
	    class: App\Model\Persistent\Repository\DifficultyRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Difficulty

	repository.subcategory:
	    class: App\Model\Persistent\Repository\SubCategoryRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\SubCategory

	repository.category:
	    class: App\Model\Persistent\Repository\CategoryRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Category

	repository.condition.type:
	    class: App\Model\Persistent\Repository\ProblemConditionTypeRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemConditionType

	repository.condition:
	    class: App\Model\Persistent\Repository\ProblemConditionRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemCondition

	repository.template.equation.linear:
	    class: App\Model\Persistent\Repository\ProblemTemplate\LinearEquationTemplateRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemTemplate\LinearEquationTemplate

	repository.template.equation.quadratic:
	    class: App\Model\Persistent\Repository\ProblemTemplate\QuadraticEquationTemplateRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemTemplate\QuadraticEquationTemplate

	repository.template.sequence.arithmetic:
	    class: App\Model\Persistent\Repository\ProblemTemplate\ArithmeticSequenceTemplateRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemTemplate\ArithmeticSequenceTemplate

	repository.template.sequence.geometric:
	    class: App\Model\Persistent\Repository\ProblemTemplate\GeometricSequenceTemplateRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\ProblemTemplate\GeometricSequenceTemplate

	repository.supergroup:
	    class: App\Model\Persistent\Repository\SuperGroupRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\SuperGroup

	repository.group:
	    class: App\Model\Persistent\Repository\GroupRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Group

	repository.logo:
	    class: App\Model\Persistent\Repository\LogoRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Logo

	repository.test:
	    class: App\Model\Persistent\Repository\TestRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\Test

	repository.test.variant:
	    class: App\Model\Persistent\Repository\TestVariantRepository
	    tags:
	        doctrine.repositoryEntity: App\CoreModule\Model\Persistent\Entity\TestVariant

	repository.problem.final.test.variant.association:
	    class: App\Model\Persistent\Repository\ProblemFinalTestVariantAssociationRepository
	    tags:
